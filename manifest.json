{
    "manifest_version": 3,
    "name": "Internet Speed Test",
    "version": "1.0",
    "description": "A Chrome extension to test internet speed.",
    "permissions": ["scripting"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  